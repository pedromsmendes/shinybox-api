{
  "name": "shinybox-api",
  "version": "0.0.1",
  "description": "Shinybox API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "env-cmd -f .env.dev nodemon",
    "lint": "echo \"Linting...\n\" && eslint src --ext .ts,.tsx,.gql --fix && echo \"Linting done\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build && tsc --project tsconfig.build.json && tsconfig-replace-paths --project tsconfig.build.json",
    "start": "env-cmd -f .env node ./buil/index.js",
    "postinstall": "npm run prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "env-cmd -f .env.dev prisma migrate dev",
    "prisma:migrate:reset": "env-cmd -f .env.dev prisma migrate reset",
    "prisma:migrate:status": "env-cmd -f .env.dev prisma migrate status",
    "prisma:migrate:deploy": "env-cmd -f .env.dev prisma migrate deploy"
  },
  "keywords": [
    "shinybox-api",
    "shinybox",
    "api"
  ],
  "author": "pedromsmendes",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.3",
    "graphql": "^15.3.0",
    "graphql-upload": "^13.0.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
    "type-graphql": "^1.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.15",
    "prisma": "^3.11.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typescript": "^4.6.2"
  }
}
